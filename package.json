  "scripts": {
    "eject": "react-scripts eject",
    "start": "node index.js",
    "start:debug": "cross-env DEBUG=auth-api-starterpack:* yarn start",
    "lint": "esw *.js server config --color",
    "lint:watch": "yarn lint -- --watch",
    "test": "yarn test:auth && yarn test:user",
    "test:auth": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors auth --recursive",
    "test:user": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors user --recursive",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors server --recursive",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "commitizen": "^2.9.6",
    "coveralls": "^3.0.0",
